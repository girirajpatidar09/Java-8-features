package com.giriraj;

import java.util.List;

public class Demo2 {
	public static void main(String ar[])
	{
		System.out.println("Print all even numbers from a list");
		List<Integer> numbers = List.of(1, 2, 3, 4, 5, 6);
		numbers.stream().filter((Integer data)-> data%2==0).forEach((Integer i)->System.out.println(i));
	}

}
