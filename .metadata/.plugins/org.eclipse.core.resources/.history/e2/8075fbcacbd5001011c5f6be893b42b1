package com.giriraj;

import java.util.List;

public class Demo3 {
	public static void main(String ar[])
	{
		System.out.println("Find names starting with letter 'A'");
		List<String> names = List.of("Amit", "Raju", "Anita", "Rohan");
		names.stream().filter((String s1)->s1.startsWith("A"))).forEach(System.out::println));
		
		
	}

}
